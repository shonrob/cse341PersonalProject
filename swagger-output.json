{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "School API",
    "description": "Documentation to get my contacts"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {},
  "definitions": {
    "course": {
      "type": "object",
      "properties": {
        "subject": {
          "type": "string",
          "example": "Math"
        },
        "description": {
          "type": "string",
          "example": "Subject Material"
        },
        "teacherName": {
          "type": "string",
          "example": "Mr/Ms Doe"
        },
        "currentGrade": {
          "type": "string",
          "example": "A"
        },
        "assignment": {
          "type": "string",
          "example": "What is the project?"
        },
        "dueDate": {
          "type": "string",
          "example": "01/01/2025"
        }
      }
    },
    "student": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string",
          "example": "Sarah"
        },
        "lastName": {
          "type": "string",
          "example": "Doe"
        },
        "enrolled": {
          "type": "boolean",
          "example": true
        }
      }
    }
  },
  "server": [
    {
      "url": "http:localhost3000",
      "description": "This is my local server."
    },
    {
      "url": "https://cse341project.onrender.com/",
      "description": "This is my render server."
    }
  ]
}